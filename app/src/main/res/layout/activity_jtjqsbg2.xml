<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ruiduoyi.activity.Jtjqsbg2Activity">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:background="@color/blue_sl_false">

        <Button
            android:id="@+id/cancle_btn"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_gravity="center_vertical|right"
            android:layout_marginRight="20dp"
            android:background="@drawable/cmdclose_sl" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="机台及腔数变更"
            android:textColor="@android:color/white"
            android:textSize="30dp" />
    </FrameLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="460dp"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="7dp"
                    android:text="原机台编号"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/jtbh_text"
                    android:layout_width="70dp"
                    android:layout_height="39dp"
                    android:layout_margin="3dp"
                    android:background="@drawable/text_bg"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/text_size" />

                <TextView
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="7dp"
                    android:text="->变更为"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_gravity="center_vertical"
                    android:id="@+id/jtbh_bg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="3dp"
                    android:layout_marginLeft="3dp">

                    <LinearLayout
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/text_bg">

                        <TextView
                            android:id="@+id/new_jtbh_text"
                            android:layout_width="70dp"
                            android:layout_height="36dp"
                            android:gravity="center_vertical"
                            android:textSize="@dimen/text_size" />
                    </LinearLayout>
                </LinearLayout>
                <Button
                    android:id="@+id/jtbh_sub"
                    android:layout_width="55dp"
                    android:text="提交"
                    android:textSize="20dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="10dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="440dp"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="440dp"
                    android:layout_height="35dp"
                    android:layout_gravity="center_horizontal"
                    android:background="@color/blue_sl_false"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="85dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:gravity="center"
                        android:text="机台编号"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:layout_width="90dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:gravity="center"
                        android:text="产出数量"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:layout_width="90dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:gravity="center"
                        android:text="良品率%"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:layout_width="175dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:gravity="center"
                        android:text="最近生产时间"
                        android:textColor="@android:color/black" />
                </LinearLayout>

                <ListView
                    android:background="@drawable/text_bg"
                    android:id="@+id/list_1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:divider="@color/touming"></ListView>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_weight="1"
            android:background="@drawable/text_bg"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="325dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="60dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="7dp"
                            android:text="模具编号"
                            android:textColor="#000000"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/mjbh_text"
                            android:layout_width="300dp"
                            android:layout_height="36dp"
                            android:layout_margin="3dp"
                            android:background="@drawable/text_bg"
                            android:gravity="center_vertical"
                            android:textSize="@dimen/text_size" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="20dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="60dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="7dp"
                            android:text="模具名称"
                            android:textColor="#000000"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/mjmc_text"
                            android:layout_width="300dp"
                            android:layout_height="36dp"
                            android:layout_margin="3dp"
                            android:background="@drawable/text_bg"
                            android:gravity="center_vertical"
                            android:textSize="@dimen/text_size" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="20dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="60dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="7dp"
                            android:text="产品腔数"
                            android:textColor="#000000"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/mjxs_text"
                            android:layout_width="110dp"
                            android:layout_height="36dp"
                            android:layout_margin="3dp"
                            android:background="@drawable/text_bg"
                            android:gravity="center_vertical"
                            android:textSize="@dimen/text_size" />

                        <TextView
                            android:id="@+id/textView6"
                            android:layout_width="60dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="7dp"
                            android:text="实际腔数"
                            android:textColor="#000000"
                            android:textSize="15dp" />

                        <LinearLayout
                            android:id="@+id/qs_bg"

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="3dp"
                                android:background="@drawable/text_bg">

                                <TextView
                                    android:id="@+id/cpxs_text"
                                    android:layout_width="110dp"
                                    android:layout_height="36dp"
                                    android:background="@null"
                                    android:gravity="center_vertical"
                                    android:textSize="@dimen/text_size" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="20dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="80dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:gravity="center"
                            android:text="堵头位置："
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="7dp"
                            android:text="第"
                            android:textColor="#000000"
                            android:textSize="15dp" />

                        <LinearLayout
                            android:layout_margin="3dp"
                            android:layout_width="90dp"
                            android:layout_height="36dp"
                            android:background="@drawable/text_bg">

                            <TextView
                                android:id="@+id/row"
                                android:layout_width="90dp"
                                android:layout_height="36dp"
                                android:gravity="center_vertical"
                                android:textSize="@dimen/text_size" />
                        </LinearLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="排"
                            android:textSize="15dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="7dp"
                            android:text="第"
                            android:textColor="#000000"
                            android:textSize="15dp" />

                       <LinearLayout
                           android:layout_margin="3dp"
                           android:layout_width="90dp"
                           android:layout_height="36dp"
                           android:background="@drawable/text_bg">
                           <TextView
                               android:id="@+id/col"
                               android:layout_width="90dp"
                               android:layout_height="36dp"
                               android:gravity="center_vertical"
                               android:textSize="@dimen/text_size" />
                       </LinearLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="列"
                            android:textSize="15dp" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="55dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="20dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="60dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="7dp"
                            android:text="堵头原因"
                            android:textColor="#000000"
                            android:textSize="15dp" />

                        <Button
                            android:id="@+id/spinner"
                            android:layout_width="305dp"
                            android:layout_height="match_parent"
                            android:background="@drawable/spnner_bg"
                            android:gravity="center_vertical|left"
                            android:textSize="20dp" />
                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="15dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/btn_1"
                            android:layout_width="@dimen/jtjqsbg_num_width"
                            android:layout_height="@dimen/jtjqsbg_num_heigth"
                            android:layout_margin="2dp"
                            android:text="1"
                            android:textSize="20dp" />

                        <Button
                            android:id="@+id/btn_2"
                            android:layout_width="@dimen/jtjqsbg_num_width"
                            android:layout_height="@dimen/jtjqsbg_num_heigth"
                            android:layout_margin="2dp"
                            android:text="2"
                            android:textSize="20dp" />

                        <Button
                            android:id="@+id/btn_3"
                            android:layout_width="@dimen/jtjqsbg_num_width"
                            android:layout_height="@dimen/jtjqsbg_num_heigth"
                            android:layout_margin="2dp"
                            android:text="3"
                            android:textSize="20dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/btn_4"
                            android:layout_width="@dimen/jtjqsbg_num_width"
                            android:layout_height="@dimen/jtjqsbg_num_heigth"
                            android:layout_margin="2dp"
                            android:text="4"
                            android:textSize="20dp" />

                        <Button
                            android:id="@+id/btn_5"
                            android:layout_width="@dimen/jtjqsbg_num_width"
                            android:layout_height="@dimen/jtjqsbg_num_heigth"
                            android:layout_margin="2dp"
                            android:text="5"
                            android:textSize="20dp" />

                        <Button
                            android:id="@+id/btn_6"
                            android:layout_width="@dimen/jtjqsbg_num_width"
                            android:layout_height="@dimen/jtjqsbg_num_heigth"
                            android:layout_margin="2dp"
                            android:text="6"
                            android:textSize="20dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/btn_7"
                            android:layout_width="@dimen/jtjqsbg_num_width"
                            android:layout_height="@dimen/jtjqsbg_num_heigth"
                            android:layout_margin="2dp"
                            android:text="7"
                            android:textSize="20dp" />

                        <Button
                            android:id="@+id/btn_8"
                            android:layout_width="@dimen/jtjqsbg_num_width"
                            android:layout_height="@dimen/jtjqsbg_num_heigth"
                            android:layout_margin="2dp"
                            android:text="8"
                            android:textSize="20dp" />

                        <Button
                            android:id="@+id/btn_9"
                            android:layout_width="@dimen/jtjqsbg_num_width"
                            android:layout_height="@dimen/jtjqsbg_num_heigth"
                            android:layout_margin="2dp"
                            android:text="9"
                            android:textSize="20dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/btn_0"
                            android:layout_width="@dimen/jtjqsbg_num_width"
                            android:layout_height="@dimen/jtjqsbg_num_heigth"
                            android:layout_margin="2dp"
                            android:text="0"
                            android:textSize="20dp" />

                        <Button
                            android:id="@+id/btn_clear"
                            android:layout_width="@dimen/jtjqsbg_num_width"
                            android:layout_height="@dimen/jtjqsbg_num_heigth"
                            android:layout_margin="2dp"
                            android:text="清除"
                            android:textSize="20dp" />

                        <Button
                            android:id="@+id/btn_submit"
                            android:layout_width="@dimen/jtjqsbg_num_width"
                            android:layout_height="@dimen/jtjqsbg_num_heigth"
                            android:layout_margin="2dp"
                            android:text="提交"
                            android:textSize="20dp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="792dp"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="792dp"
                    android:layout_height="35dp"
                    android:background="@color/blue_sl_false"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:gravity="center"
                        android:text="堵头位置"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="180dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:gravity="center"
                        android:text="堵头原因"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:layout_width="75dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:gravity="center"
                        android:text="建立人员"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:layout_width="126dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:gravity="center"
                        android:text="建立日期"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:id="@+id/textView11"
                        android:layout_width="75dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:gravity="center"
                        android:text="确认人员"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:layout_width="126dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:gravity="center"
                        android:text="确认日期"
                        android:textColor="@android:color/black" />
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/text_bg"
                        android:gravity="center"
                        android:textColor="@android:color/black" />
                </LinearLayout>

                <ListView
                    android:id="@+id/list_2"
                    android:layout_marginBottom="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:divider="@color/touming"></ListView>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
